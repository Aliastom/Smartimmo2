{
  "name": "smartimmo-greenfield",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "prisma generate && next dev",
    "dev:pg": "cross-env DATABASE_URL=postgresql://smartimmo:smartimmo@localhost:5432/smartimmo?schema=public npm run dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:ui": "vitest",
    "test:e2e": "playwright test",
    "test:ai": "vitest run tests/ai/acceptance.test.ts",
    "test:ai-quick": "tsx scripts/test-ai-acceptance.ts",
    "test:competence-b": "vitest run tests/ai/competence-b.test.ts",
    "test:competence-b-quick": "tsx scripts/test-competence-b.ts",
    "test:competence-a": "tsx scripts/test-competence-a.ts",
    "test:competence-c": "tsx scripts/test-competence-c.ts",
    "test:competence-d": "tsx scripts/test-competence-d.ts",
    "test:competence-d-complete": "tsx scripts/test-competence-d-complete.ts",
    "test:competence-e": "tsx scripts/test-competence-e.ts",
    "test:competence-e-complete": "tsx scripts/test-competence-e-plan-complet.ts",
    "test:all-competences": "npm run test:competence-a && npm run test:competence-b-quick && npm run test:competence-c && npm run test:competence-d-complete && npm run test:competence-e-complete",
    "test:memory": "tsx scripts/test-conversation-memory.ts",
    "services:start": "docker-compose up -d",
    "services:stop": "docker-compose stop",
    "services:down": "docker-compose down",
    "services:restart": "docker-compose restart",
    "services:logs": "docker-compose logs -f",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed-documents": "node scripts/seed-documents-system.js",
    "db:seed-document-types": "node scripts/seed-document-types.js",
    "db:seed-document-types-unified": "tsx prisma/seeds/document-types-unified.ts",
    "db:test-document-admin": "node scripts/test-document-admin.js",
    "seed:nature-mapping": "tsx scripts/seed-nature-mapping.ts",
    "seed:nature-mapping:reset": "tsx scripts/seed-nature-mapping.ts reset",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "postinstall": "prisma generate",
    "db:setup": "docker-compose up -d postgres",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:diff": "prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --script",
    "db:migrate:data": "DATABASE_URL_SQLITE='file:./prisma/dev.db' tsx scripts/migrate-sqlite-to-postgres.ts",
    "db:check": "DATABASE_URL_SQLITE='file:./prisma/dev.db' tsx scripts/check-integrity.ts",
    "db:studio": "prisma studio",
    "scan:fr": "tsx tools/scan-french-identifiers.ts",
    "codemod:dry": "tsx tools/codemod-identifiers.ts --dry",
    "codemod:write": "tsx tools/codemod-identifiers.ts --write",
    "lint:guard": "node tools/guard-french-identifiers.js",
    "lint-theme": "node scripts/lint-theme.js",
    "lint-theme-safety": "node scripts/lint-theme-safety.js",
    "fix-theme-violations": "node scripts/fix-theme-violations.js",
    "replace-colors:dry": "node scripts/replace-hardcoded-colors.js --dry",
    "replace-colors": "node scripts/replace-hardcoded-colors.js",
    "ingest:kb": "tsx scripts/ingest/ingest_kb.ts",
    "ingest:code": "tsx scripts/ingest/ingest_code.ts",
    "ingest:schemas": "tsx scripts/ingest/ingest_schemas.ts",
    "ingest:all": "tsx scripts/ingest/ingest_all.ts",
    "kb:truncate": "tsx scripts/ingest/ingest_kb.ts --truncate",
    "kb:rebuild": "npm run kb:truncate && npm run ingest:all",
    "check:env": "tsx scripts/check-env.ts",
    "db:migrate:ai": "tsx scripts/migrate-ai-tables.ts",
    "db:migrate:ai-log": "tsx scripts/migrate-ai-query-log.ts",
    "db:seed:ai": "tsx prisma/seeds/ai-analytics-seed.ts",
    "db:views": "tsx scripts/apply-analytics-views.ts",
    "ai:catalog": "tsx scripts/generate-sql-catalog.ts",
    "ai:config-rent": "tsx scripts/configure-rent-nature.ts",
    "ai:setup": "npm run db:migrate:ai && npm run db:migrate:ai-log && npm run db:seed:ai && npm run ai:config-rent && npm run db:views && npm run ai:catalog"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.11.1",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^6.18.0",
    "@qdrant/js-client-rest": "^1.11.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-pdf/renderer": "^4.3.1",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/uuid": "^10.0.0",
    "@xenova/transformers": "^2.17.2",
    "adm-zip": "^0.5.16",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.20",
    "canvas": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "daisyui": "^5.2.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.454.0",
    "mammoth": "^1.11.0",
    "n2words": "^1.23.0",
    "next": "^14.2.10",
    "next-auth": "4.24.13",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.11.174",
    "pgsql-ast-parser": "^12.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.65.0",
    "react-hot-toast": "^2.6.0",
    "react-signature-canvas": "^1.1.0-alpha.2",
    "recharts": "^2.12.7",
    "resend": "^6.1.2",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tesseract.js": "^5.1.1",
    "uuid": "^13.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/adm-zip": "^0.5.7",
    "@types/archiver": "^6.0.2",
    "@types/node": "^20.12.14",
    "@types/react": "^18.3.7",
    "@vitest/coverage-v8": "^2.1.3",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cross-env": "^10.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.10",
    "eslint-config-prettier": "^10.1.8",
    "glob": "^11.0.3",
    "jsdom": "^27.0.0",
    "playwright": "^1.48.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prisma": "^6.18.0",
    "tailwindcss": "^3.4.13",
    "ts-morph": "^22.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
